{
  "version": 2,
  "builds": [
    {
      "src": "book/package.json",
      "use": "@vercel/static-build",
      "config": {
        "rootDir": "book",
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}